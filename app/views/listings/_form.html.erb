<%= simple_form_for(@listing) do |f| %>
  <%= f.error_notification %>
    <%= f.association :medical_condition, collection: MedicalCondition.all, value_method: :id, input_html: {class: "form-control"} %>
    <%= f.input :comments,input_html: {class: "form-control"}%>
    <%= f.input :photo_medical_data, input_html: {class: "form-control"} %>
    <br/>
    <%= f.button :submit, input_html: {class: "form-control"} %>

<% end %>
