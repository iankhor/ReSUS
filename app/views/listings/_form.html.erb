<%= simple_form_for(@listing) do |f| %>
  <%= f.error_notification %>


  <div class="form-inputs">
    <%= f.association :medical_condition, collection: MedicalCondition.all, value_method: :id %>
    <%= f.input :comments %>
    <%= f.input :photo_medical_data %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
