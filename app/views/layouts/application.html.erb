<!DOCTYPE html>
<html>
  <head>
    <title>ReSUS - Healthcare for everyone</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <!-- Custom Fonts -->
    <%= stylesheet_link_tag 'https://fonts.googleapis.com/css?family=Lato' %>
    <%= stylesheet_link_tag 'https://fonts.googleapis.com/css?family=Catamaran:100,200,300,400,500,600,700,800,900' %>
    <%= stylesheet_link_tag 'https://fonts.googleapis.com/css?family=Muli' %>

    <!-- Plugin CSS -->
    <%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/simple-line-icons/2.4.1/css/simple-line-icons.css' %>
    <%= stylesheet_link_tag 'https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css' %>


    <!-- Theme CSS -->
    <%= stylesheet_link_tag 'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css' %>
    <%= stylesheet_link_tag 'new-age.min.css' %>

    <!--My CSS-->
    <%= stylesheet_link_tag 'my_css.css' %>

  </head>

  <body >

    <%= render 'layouts/navbar' %>

    <%= yield %> 
    

    <!-- jQuery -->
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js' %>

    <!-- Bootstrap Core JavaScript -->
    <%= javascript_include_tag 'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js', integrity: 'Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa' , crossorigin: 'anonymous'   %>

    <!-- Plugin JavaScript -->
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js' %>
    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js' %>

    <!-- Theme JavaScript -->
    <%= javascript_include_tag 'new-age.min.js' %>


    <%= render 'footer.html'%>

  </body>


</html>
